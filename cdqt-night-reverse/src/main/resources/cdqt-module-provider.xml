<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="mysql" defaultModelType="flat" targetRuntime="MyBatis3Simple">

    <property name="autoDelimitKeywords" value="false" />
    <property name="javaFileEncoding" value="UTF-8" />
    <!-- 注释 -->
    <!-- 自定义注释生成器 -->
    <commentGenerator type="org.cdqt.night.reverse.comment.CommentGenerator">
      <property name="author" value="LiuGangQiang" />
      <property name="dateFormat" value="yyyy/MM/dd" />
    </commentGenerator>
    <!-- <commentGenerator> <property name="suppressDate" value="true" /> <property name="suppressAllComments" value="true" /> </commentGenerator> -->

    <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver" connectionURL="jdbc:mysql://192.168.1.4:3306/cdqt"
      userId="cdqt" password="123456">
    </jdbcConnection>

    <javaTypeResolver type="org.mybatis.generator.internal.types.JavaTypeResolverDefaultImpl">
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>


    <javaModelGenerator targetPackage="org.cdqt.module.provider.entity" targetProject="cdqt-module-provider">
      <property name="rootClass" value="org.cdqt.night.core.entity.BaseEntity" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="org.cdqt.module.provider.mapper" targetProject="cdqt-module-provider">
      <property name="enableSubPackages" value="ture" />
    </sqlMapGenerator>
    <javaClientGenerator type="XMLMAPPER" targetPackage="org.cdqt.module.provider.mapper"
      targetProject="cdqt-module-provider">
      <property name="rootInterface" value="org.cdqt.night.core.mapper.IBaseMapper" />
      <property name="enableSubPackages" value="ture" />
    </javaClientGenerator>

    <table tableName="base_%" enableDeleteByPrimaryKey="false" enableSelectByPrimaryKey="false"
      enableUpdateByPrimaryKey="false" >
      <domainObjectRenamingRule searchString="^Base" replaceString="" />
    </table>
  </context>
</generatorConfiguration>
